@model GesStaDemo.Models.Entities.MaitreDeStage

@{
    ViewBag.Title = "Modification d'un maître de stage";
    Layout = "~/Views/Admin/_AdminLayout.cshtml";
}


<div>
    
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Section")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Section.LibSec)
        </dd>

        <dt>
            @Html.DisplayName("Code utilisateur")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Utilisateur.Login)
        </dd>

        <dt>
            @Html.DisplayName("Nom")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.NomMS)
        </dd>

        <dt>
            @Html.DisplayName("Prénom")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.PrenMS)
        </dd>

        <dt>
            @Html.DisplayName("Téléphone")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TelMS)
        </dd>

        <dt>
            @Html.DisplayName("Email")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.AdrMS)
        </dd>

        <dt>
            @Html.DisplayNameFor(model => model.Fonction)
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Fonction)
        </dd>


    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

<div class="form-actions no-color">
    <input type="submit" value="Supprimer" class="btn btn-danger" />
    <a href="" type="reset" value="Annuler" class="btn btn-default"><i class="fa fa-redo"></i></a>
    <a href="@Url.Action("Index","MaitreDeStage")" type="submit" value="" class="btn btn-default"><i class="fa fa-arrow-left"></i></a>
</div>
    }
</div>
